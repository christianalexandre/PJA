<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".ResultActivity">

    <Toolbar
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:layout_marginTop="-5dp"
        android:layout_marginStart="-15dp"
        android:layout_marginBottom="-5dp"
        android:layout_marginEnd="-15dp"
        app:layout_constraintBottom_toBottomOf="@id/Title"
        app:layout_constraintEnd_toEndOf="@id/Title"
        app:layout_constraintStart_toStartOf="@id/Title"
        app:layout_constraintTop_toTopOf="@+id/Title"
        />

    <Toolbar
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="-10dp"
        android:layout_marginEnd="-10dp"
        app:layout_constraintTop_toTopOf="@+id/Title"
        app:layout_constraintStart_toStartOf="@id/Title"
        app:layout_constraintBottom_toBottomOf="@id/Title"
        app:layout_constraintEnd_toEndOf="@id/Title"
        android:background="@color/white"
        />

    <TextView
        android:id="@+id/Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_second_activity"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/toolbar"
        android:textSize="30sp"
        />

    <Toolbar
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:layout_marginTop="1dp"
        android:layout_marginStart="-1dp"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="-1dp"
        app:layout_constraintBottom_toBottomOf="@id/buttonReturn"
        app:layout_constraintEnd_toEndOf="@id/buttonReturn"
        app:layout_constraintStart_toStartOf="@id/buttonReturn"
        app:layout_constraintTop_toTopOf="@+id/buttonReturn"
        />

    <Button
        android:id="@+id/buttonReturn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_return"
        android:backgroundTint="@color/white"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        />

    <Toolbar
        android:id="@+id/toolbar1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:backgroundTint="@color/black"
        android:layout_marginEnd="-5dp"
        android:layout_marginBottom="-5dp"
        android:layout_marginTop="-5dp"
        android:layout_marginStart="-5dp"
        app:layout_constraintStart_toStartOf="@id/toolbar"
        app:layout_constraintEnd_toEndOf="@id/toolbar"
        app:layout_constraintTop_toTopOf="@id/toolbar"
        app:layout_constraintBottom_toBottomOf="@id/toolbar"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        >

        <TextView
            android:id="@+id/sinal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sinal_equals"
            android:textSize="35dp"
            app:layout_constraintTop_toTopOf="@id/toolbar"
            app:layout_constraintBottom_toBottomOf="@id/toolbar"
            app:layout_constraintStart_toStartOf="@id/toolbar"
            app:layout_constraintEnd_toEndOf="@id/toolbar"
            android:layout_marginBottom="4dp"
            android:textColor="@color/black"
            />

        <TextView
            android:id="@+id/coin1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1000 BRL"
            android:textColor="@color/black"
            android:textSize="17sp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            app:layout_constraintBottom_toBottomOf="@id/toolbar"
            app:layout_constraintTop_toTopOf="@id/toolbar"
            app:layout_constraintEnd_toStartOf="@id/sinal"
            app:layout_constraintStart_toStartOf="@+id/toolbar"
            />

        <TextView
            android:id="@+id/coinResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1000 USD"
            android:textColor="@color/black"
            android:textSize="17sp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintStart_toEndOf="@id/sinal"
            app:layout_constraintBottom_toBottomOf="@id/toolbar"
            app:layout_constraintTop_toTopOf="@id/toolbar"
            app:layout_constraintEnd_toEndOf="@+id/toolbar"
            />

        <ImageView
            android:id="@+id/imgBR1"
            android:layout_width="50sp"
            android:layout_height="75dp"
            app:srcCompat="@drawable/flag_brazil"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="206dp"
            app:layout_constraintTop_toTopOf="@id/toolbar"
            app:layout_constraintBottom_toBottomOf="@id/toolbar"
            app:layout_constraintStart_toStartOf="@id/toolbar"
            app:layout_constraintEnd_toStartOf="@id/coin1"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:visibility="gone"
            />

        <ImageView
